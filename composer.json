{
    "name": "uma/dic",
    "description": "A minimalistic PSR-11 container",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "UMA\\DIC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UMA\\DIC\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit"
    }
}
